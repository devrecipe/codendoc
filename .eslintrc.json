{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": ["error","always"],
        "quotes": ["error","single"],
        "no-console": "off",
        "no-unused-vars": "warn"
    },
    "parserOptions": {
        "ecmaVersion": 6
    }
}